bind generic i move-up
bind generic k move-down
bind generic ^I scroll-line-up
bind generic ^K scroll-line-down
bind generic ^A move-first-line
bind generic ^E move-last-line
bind generic j toggle-sort-field
bind generic ; prompt
bind generic tab view-next

bind main B !git rebase -i %(commit)^
bind main v !git v %(commit)
bind main space enter

set focus-child = false
